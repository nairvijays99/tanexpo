{
  "name": "tanexpo-monorepo",
  "version": "0.0.0",
  "private": true,
  "description": "Share UI components between Expo Native and TanStack Start",
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/tanexpo"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "clean": "turbo run clean && rm -rf node_modules",
    "lint": "biome lint .",
    "lint:fix": "biome lint --apply .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "check": "biome check .",
    "check:fix": "biome check --apply ."
  },
  "keywords": [
    "expo",
    "tanstack",
    "react",
    "react-native",
    "cross-platform"
  ],
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "tsup": "^8.5.1",
    "turbo": "^2.7.0",
    "typescript": "^5.9.3"
  }
}